@model IEnumerable<ClinicV2.Models.Account>

@{
    ViewBag.Title = "ListAccount";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Admin Account</h2>

<p>
    @Html.ActionLink("Create New", "CreateAccount")
</p>
<table class="table table-striped table-hover table-bordered">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.AdminID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Username)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FirstName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LastName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.AdminID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Username)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FirstName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LastName)
        </td>
        <td>
            @Html.ActionLink("Edit", "EditAccount", new { id = item.AdminID }) |
            @Html.ActionLink("Delete", "DeleteAccount", new {id= item.AdminID })
        </td>
    </tr>
}

</table>
