@model  IEnumerable<ClinicV2.Models.clinicModel>

@{
    ViewBag.Title = "GetRequirement";
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css">
}
    <head>
        <script src="https://unpkg.com/axios/dist/axios.min.js"></script>
    </head>
    <style>
        #map {
        height:400px;
        width:100%;
        }

    </style>



<body>
    <script src="~/Scripts/CustScript/MapScript.js"></script>
    <hr>
    <div>
   
        <div>
            <table class="table table-striped table-hover table-bordered" id="list_Of_Clinic" >
                <tr>

                    <th scope="col">Clinic Name</th>
                    <th scope="col">Address</th>
                    <th scope="col">Phonenumber</th>
                    <th scope="col">Distance</th>
                </tr>

                @foreach (var item in Model)
                {

                <tr id="@item.Name" name="clinicOption">
                    <td scope="row"><input type="checkbox" name="clinicName" id="ID_@item.Name" value="@item.ClinicID" onclick="geocode('@item.Address','ID_@item.Name','fromList')"> <a href="http://@item.Website" target="_blank">@item.Name</a></td>
                    <td scope="row"> @item.Address </td>     
                    <td scope="row"> @item.PhoneNumber</td>
                    <td id="Distance_@item.Name" scope="row"></td>
                </tr>
                }
            </table>

        </div>
    </div>
    <hr>


</body>


